{
	"schemaVersion": 1,
	"id": "${mod_id}",
	"version": "${mod_version}",
	"name": "${mod_name}",
	"description": "${mod_description}",
	"authors": [
		"${mod_author}"
	],
	"contributors": [
		"${mod_credits_1}",
		"${mod_credits_2}"
	],
	"contact": {
		"homepage": "${mod_url}"
	},
